{
    "name": "denysmotion/http-framework",
    "type": "project",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Framework\\": "src/Framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\App\\": "tests/App/",
            "Test\\Framework\\": "tests/Framework/"
        }
    },
    "authors": [
        {
            "name": "denysmotion"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
